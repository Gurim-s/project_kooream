<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.kooream.mapper.OriginalAttachMapper">
	<insert id="insert">
		INSERT INTO original_attach(uuid, uploadPath, fileName, fileType, orino)
		VALUES (#{uuid}, #{uploadPath}, #{fileName}, #{fileType}, #{orino})
	</insert>
	
	<delete id="delete">
		DELETE FROM original_attach WHERE uuid = #{uuid}
	</delete>
	
	<select id="findByBno" resultType="com.kooream.domain.OriginalAttachVO">
		SELECT * FROM original_attach WHERE orino = #{orino}
	</select>

</mapper>